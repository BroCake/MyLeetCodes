import java.util.Stack;

class Solution {
  
    public boolean isPalindrome(int x) {

        boolean ans = true;
        if (x < 0) {
            return false;
        }

        int a = x, digits = 0;
        Stack<Integer> stk = new Stack<>();

        while (a > 0) {
            a /= 10;
            digits++;
        }

        int b = digits / 2;
        a = x;

        while (b > 0) {
            stk.push(a % 10);
            a /= 10;
            b--;
        }

        if (digits % 2 == 1) {
            stk.push(a % 10);
        }

        while (a > 0) {

            if (a % 10 != stk.pop()) {
                ans = false;
                a = 0;
            }
            a /= 10;
        }

        System.out.println(ans);
        return ans;
    }
}
